import java.util.*;
public class Main
{
    public static void findSum(int m,int n)
    {
        int sum=0,diff=0;
        for(int i=1;i<=m;i++)
        {
            if(i % n==0)
            {
                sum +=i;
            }else
            {
                diff +=i;
            }
        }
        System.out.println(diff-sum);
    }
	public static void main(String[] args) {
	   int m=20;
     int n=4;
     findSum(m,n);
	}
}
